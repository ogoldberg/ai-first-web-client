{
  "name": "@unbrowser/mcp",
  "version": "0.1.0-alpha.1",
  "description": "MCP server for Unbrowser - Model Context Protocol interface",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "unbrowser": "dist/index.js"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
